# ENDSTOPS and HOMING

# X endstop
endstop.minx.axis                            X
endstop.minx.enable                          true
endstop.minx.limit_enable                    false
endstop.minx.pin                             1.24!^
endstop.minx.homing_direction                home_to_max
endstop.minx.homing_position                 395
endstop.minx.max_travel                      400
endstop.minx.fast_rate                       50
endstop.minx.slow_rate                       8
endstop.minx.retract                         0

# Y endstop
endstop.miny.axis                            Y
endstop.miny.enable                          true
endstop.miny.limit_enable                    false
endstop.miny.pin                             1.25!^
